class FindMissingNumber  {
    public static void main(String[] args) {
        //Write a Java program to find the missing number in an array of integers from 1 to n
        int arr[]={1,2,3,5,6,7,8};
        int n=8;
        int totalSum=n*(n+1)/2;// toal sum of the number
        int currentSum=0;
        for(int num:arr){
            currentSum+=num;
        }
        System.out.println("totalSum is="+(totalSum));
        System.out.println("currentSum is="+(currentSum));
        System.out.println("Missing number is="+(totalSum-currentSum));
    }
      
     
}
